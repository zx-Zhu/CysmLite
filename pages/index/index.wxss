/**index.wxss**/


.search {
  display: flex;
  width: 60%;  
  height: 80rpx;
  border: 1px solid #d0d0d0;  
  border-radius: 10rpx;  
  align-items: center;
  padding: 10rpx;
  padding-left: 20rpx;
  margin-top: 200rpx;
}
.search_btn {
  display: flex;
  color: white;
  background: #9ECE6F;
  width: 200rpx;
  height: 200rpx;
  margin: 20rpx;
  border-radius: 50%;
  align-items: center;
  justify-content: center; 
  margin-top: 200rpx;
}
.hello_btn {
  display: flex;
  color: white;
  background: #9ECE6F;
  width: 200rpx;
  height: 200rpx;
  margin: 20rpx;
  border-radius: 50%;
  align-items: center;
  justify-content: center; 
  margin-top: 200rpx;
}

.btn-hover {
  background: #8EB964;
}
.hello-hover {
  background: #8EB964;
}
